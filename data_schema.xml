      <description>Description TBD</description>
      <lookup>
        <term> {Sunny}
          <description>Description TBD</description>
        </term>
        <term> {Cloudy}
          <description>Description TBD</description>
        </term>
        <term> {Stormy}
          <description>Description TBD</description>
        </term>
        <term> {Rain}
          <description>Description TBD</description>
        </term>
        <term> {Lots_of_Rain}
          <description>Description TBD</description>
        </term>
