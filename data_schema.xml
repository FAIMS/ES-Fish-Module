    <property name="Fishing Method" type="enum">
      <description>Description TBD</description>
      <lookup>
        <term> {No_Observation}
          <description>Description TBD</description>
        </term>
        <term> {Bait}
          <description>Description TBD</description>
        </term>
        <term> {Lure}
          <description>Description TBD</description>
