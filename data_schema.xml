      <description>Description TBD</description>
    </property>
    <property name="Released?" type="enum">
      <description>Description TBD</description>
      <lookup>
        <term> {Yes}
          <description>Description TBD</description>
        </term>
        <term> {No}
          <description>Description TBD</description>
        </term>
      </lookup>
    </property>
